<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" >

    <solid android:color="#FFFFFF" />

    //设置边距
    <padding
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp"
        android:top="10dp" />

    //边框的宽度以及颜色
    <stroke
        android:width="1dp"
        android:color="#FF3F30"
        android:dashWidth="6dp"
        android:dashGap="3dp"
        />

    //控制圆角大小
    <corners android:radius="5dp" />

</shape>
